@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ai: <http://dig.csail.mit.edu/2014/PrivacyInformer/appinventor#> .
@prefix android: <http://dig.csail.mit.edu/2014/PrivacyInformer/android#> .
@prefix camera: <http://dig.csail.mit.edu/2014/PrivacyInformer/Camera#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# camera is a component

# Component Description
camera:CameraComponent  
    a   owl:Class;
    rdfs:label  "camera";
    rdfs:subClassOf ai:DataGatheringComponent;
    ai:description  "take a picture using the device's camera";

# Privacy Implication and Common Usage
    ai:example      "Snapchat";
    ai:purpose      ai:DataGathering. 

